{
    "name": "ddeboer/data-import-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony2 bundle that offers a way to import data from and store data to a range of formats and media",
    "keywords": [ "csv", "import" ],
    "homepage": "https://github.com/ddeboer/DdeboerDataImportBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "David de Boer",
            "email": "david@ddeboer.nl"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "symfony/symfony": "dev-master"
    },
    "suggest": {
        "phpexcel/phpexcel": "*"
    },
    "autoload": {
        "psr-0": { "Ddeboer\\DataImportBundle": "" }
    },
    "target-dir": "Ddeboer/DataImportBundle"
}